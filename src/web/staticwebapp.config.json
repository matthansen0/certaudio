{
  "routes": [
    {
      "route": "/study.html",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/api/chat",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/api/chat/*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/api/me/*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/api/*",
      "allowedRoles": [
        "anonymous",
        "authenticated"
      ]
    },
    {
      "route": "/.auth/login/github",
      "statusCode": 404
    },
    {
      "route": "/.auth/login/twitter",
      "statusCode": 404
    },
    {
      "route": "/.auth/login/google",
      "statusCode": 404
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/css/*",
      "/js/*",
      "/api/*"
    ]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad",
      "statusCode": 302
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; media-src 'self' blob: https://*.blob.core.windows.net; connect-src 'self'"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".mp3": "audio/mpeg"
  }
}
